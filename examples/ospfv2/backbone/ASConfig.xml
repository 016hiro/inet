<?xml version="1.0"?>
<OSPFASConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="OSPF.xsd">

  <!-- Areas -->
  <Area id="0.0.0.0">
    <AddressRange address="R1>N1" mask="R1>N1/" status="Advertise"/>
    <AddressRange address="R1>N2" mask="R1>N2/" status="Advertise"/>
