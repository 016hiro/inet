<?xml version="1.0"?>
<!DOCTYPE checker SYSTEM "../../../etc/MessageChecker.dtd">
<!-- checker xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="MessageChecker.xsd" -->
<checker>
    <message occurence="3"/> <!-- CONNECTION_RESET, CLOSED, ESTABLISHED -->
    <message occurence="1">
        <fieldValue name="className" value="inet::BGPOpenMessage"/>
    </message>
    <message occurence="1">
        <fieldValue name="className" value="inet::BGPKeepAliveMessage"/>
    </message>
    <message occurence="1">
        <fieldValue name="className" value="inet::BGPUpdate"/>
    </message>
</checker>
