# Name of target to be created (-o option)
TARGET = TCPv4

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# uncomment 1 of the 3 lines to support either serial or parallel operation
KERNEL_LIBS=$(STD_KERNEL_LIBS)
# KERNEL_LIBS=$(PVM_KERNEL_LIBS)
# KERNEL_LIBS=$(MPI_KERNEL_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../../Network/IPv4/Core -I../../Applications/TCPApp -I../../Util

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with
EXT_DIR_OBJS=

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../../ipsuiteconfig
include ../../ipsuiteconfig

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS= TcpIpInterface_n.o tcpmodule_n.o  Ip2Tcp.o pcb.o Tcp2Ip.o tcpmodule.o TransportInterfacePacket.o TransportPacket.o

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) subdirs Makefile
	

purify: $(OBJS) $(EXTRA_OBJS) subdirs Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

TcpIpInterface_n.o: TcpIpInterface_n.cc
	$(CXX) -c $(NEDCOPTS) TcpIpInterface_n.cc

TcpIpInterface_n.cc: TcpIpInterface.ned
	$(NEDC) $(INCLUDE_PATH) TcpIpInterface.ned

tcpmodule_n.o: tcpmodule_n.cc
	$(CXX) -c $(NEDCOPTS) tcpmodule_n.cc

tcpmodule_n.cc: tcpmodule.ned
	$(NEDC) $(INCLUDE_PATH) tcpmodule.ned

Ip2Tcp.o: Ip2Tcp.cc
	$(CXX) -c $(COPTS) Ip2Tcp.cc

pcb.o: pcb.cc
	$(CXX) -c $(COPTS) pcb.cc

Tcp2Ip.o: Tcp2Ip.cc
	$(CXX) -c $(COPTS) Tcp2Ip.cc

tcpmodule.o: tcpmodule.cc
	$(CXX) -c $(COPTS) tcpmodule.cc

TransportInterfacePacket.o: TransportInterfacePacket.cc
	$(CXX) -c $(COPTS) TransportInterfacePacket.cc

TransportPacket.o: TransportPacket.cc
	$(CXX) -c $(COPTS) TransportPacket.cc


doc: neddoc doxy

neddoc:
	opp_neddoc -a

doxy: doxy.cfg
	doxygen doxy.cfg

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h $(TARGET)$(EXE_SUFFIX)
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done 

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc

re-makemake:
	opp_makemake -f  -n -r -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Applications/TCPApp -I../../Util   #recreate Makefile

re-makemake-m:
	opp_makemake -f -m  -n -r -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Applications/TCPApp -I../../Util   #recreate Makefile.in


