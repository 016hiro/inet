#
#  Makefile for NewTCP
#
#  ** This file was automatically generated by the command:
#  opp_makemake -f -N -n -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Base -I../../Util
#

# Name of target to be created (-o option)
TARGET = NewTCP

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../../Network/IPv4/Core -I../../Base -I../../Util

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with (wildcard needed to prevent "no such file *.o" errors)
EXT_DIR_OBJS=$(wildcard )

# time stamps of other directories (used as dependency)
EXT_DIR_TSTAMPS=

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../../ipsuiteconfig
include ../../ipsuiteconfig

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS= TCPCommand_m.o TCPSegment_m.o PlainTCP.o TCPConnectionBase.o TCPConnectionEventProc.o TCPConnectionRcvSegment.o TCPConnectionUtil.o TCPMain.o TCPSocket.o TCPVirtualDataRcvQueue.o TCPVirtualDataSendQueue.o

# header files generated (from msg files)
GENERATEDHEADERS= TCPCommand_m.h TCPSegment_m.h

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) Makefile
	echo>.tstamp
	
$(OBJS) : $(GENERATEDHEADERS)


purify: $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_TSTAMPS) Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

TCPCommand_m.o: TCPCommand_m.cc
	$(CXX) -c $(NEDCOPTS) TCPCommand_m.cc

TCPCommand_m.cc TCPCommand_m.h: TCPCommand.msg
	$(MSGC) $(INCLUDE_PATH) TCPCommand.msg

TCPSegment_m.o: TCPSegment_m.cc
	$(CXX) -c $(NEDCOPTS) TCPSegment_m.cc

TCPSegment_m.cc TCPSegment_m.h: TCPSegment.msg
	$(MSGC) $(INCLUDE_PATH) TCPSegment.msg

PlainTCP.o: PlainTCP.cc
	$(CXX) -c $(COPTS) PlainTCP.cc

TCPConnectionBase.o: TCPConnectionBase.cc
	$(CXX) -c $(COPTS) TCPConnectionBase.cc

TCPConnectionEventProc.o: TCPConnectionEventProc.cc
	$(CXX) -c $(COPTS) TCPConnectionEventProc.cc

TCPConnectionRcvSegment.o: TCPConnectionRcvSegment.cc
	$(CXX) -c $(COPTS) TCPConnectionRcvSegment.cc

TCPConnectionUtil.o: TCPConnectionUtil.cc
	$(CXX) -c $(COPTS) TCPConnectionUtil.cc

TCPMain.o: TCPMain.cc
	$(CXX) -c $(COPTS) TCPMain.cc

TCPSocket.o: TCPSocket.cc
	$(CXX) -c $(COPTS) TCPSocket.cc

TCPVirtualDataRcvQueue.o: TCPVirtualDataRcvQueue.cc
	$(CXX) -c $(COPTS) TCPVirtualDataRcvQueue.cc

TCPVirtualDataSendQueue.o: TCPVirtualDataSendQueue.cc
	$(CXX) -c $(COPTS) TCPVirtualDataSendQueue.cc


#doc: neddoc doxy

#neddoc:
#	opp_neddoc -a

#doxy: doxy.cfg
#	doxygen doxy.cfg

generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h $(TARGET)$(EXE_SUFFIX) .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -N -n -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Base -I../../Util 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefiles) || exit 1; done

makefile-ins:
	# recreate Makefile.in
	opp_makemake -f -m  -N -n -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Base -I../../Util 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

# "re-makemake" and "re-makemake-m" are deprecated, historic names of the above two targets
re-makemake: makefiles
re-makemake-m: makefile-ins


