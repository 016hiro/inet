
# filename: client3.irt
# routing table client no. 3 of TCP Client/Server network
# author: Jochen Reber


ifconfig:

# PPP link 0 to server 1
name: ppp0	inet_addr: 10.0.0.3	MTU: 1500	Metric: 1

# PPP link 1 to client 2
name: ppp1	inet_addr: 10.0.0.7	MTU: 1500	Metric: 1

# PPP link 1 to client 4
name: ppp2	inet_addr: 10.0.0.11	MTU: 1500	Metric: 1

ifconfigend.

route:
10.0.0.1		*				255.255.255.255 H	0	ppp0
10.0.0.2		*				255.255.255.255 H	0	ppp1
10.0.0.4		*				255.255.255.255 H	0	ppp2

routeend.

