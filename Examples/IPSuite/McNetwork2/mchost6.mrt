
# filename: mchost6.mrt
# routing table for host 6 of multicast network 2
# author: Jochen Reber


ifconfig:

# ethernet card 0 to router
name: eth0  inet_addr: 172.0.2.2   MTU: 1500   Metric: 1  BROADCAST MULTICAST
Groups: 225.0.0.3:225.0.1.2:225.0.2.1

ifconfigend.

route:

default:        172.0.2.4       0.0.0.0         G   0   eth0
225.0.0.0       172.0.2.4       255.0.0.0       G   0   eth0

routeend.

