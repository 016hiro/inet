
# filename: mchost5.mrt
# routing table for host 5 of multicast network 2
# author: Jochen Reber


ifconfig:

# ethernet card 0 to router
name: eth0	inet_addr: 172.0.2.1
Groups: 225.0.0.3:225.0.1.1:225.0.2.1
MTU: 1500	Metric: 1  BROADCAST MULTICAST

ifconfigend.

route:

default:		172.0.2.3 		0.0.0.0			G	0	eth0
225.0.0.0		172.0.2.3		255.0.0.0		G	0	eth0

routeend.

