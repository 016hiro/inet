
# filename: mchost4.mrt
# routing table for host 4 of multicast network
# author: Jochen Reber


ifconfig:

# ethernet card (modelled by point-to-point link) 4 to router 2
name: ppp0  inet_addr: 10.0.1.2  Mask: 255.255.255.0  MTU: 1500   Metric: 1  BROADCAST MULTICAST
Groups: 230.0.0.1:230.0.1.1

ifconfigend.

route:
10.0.1.2        *               255.255.255.255 H   0   ppp0
default:        10.0.0.4        0.0.0.0         G   0   ppp0
224.0.0.0       10.0.1.1        224.0.0.0       G   0   ppp0


routeend.

