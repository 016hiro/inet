
# file name: peer2nw_2.irt
# routing table for peer2 in Peer2Network
# author: Jochen Reber


ifconfig:

# ethernet card 0 to peer 1
name: eth0		encap: Ethernet		HWaddr: 00:90:27:CB:DC:2	
inet_addr: 10.0.0.2 	Bcast: 10.10.255.255 	Mask: 255.255.255.0	
MTU: 1500	Metric: 1 BROADCAST MULTICAST 

ifconfigend.

route:

10.0.0.1		*				255.255.255.255	H	0	eth0
default:		10.0.0.1		0.0.0.0			G	0	eth0

routeend.

