
# filename: peer2nw_1.irt
# routing table for peer1 in network Peer2Network
# author: Jochen Reber


ifconfig:

# ethernet card 0 to peer 2
name: eth0		encap: Ethernet		HWaddr: 00:90:27:CB:BC:2	
inet_addr: 10.0.0.1 	Bcast: 10.10.255.255 	Mask: 255.255.255.0	
MTU: 1500	Metric: 1 BROADCAST MULTICAST 

ifconfigend.

route:

10.0.0.2		*				255.255.255.255	H	0	eth0
default:		10.0.0.2		0.0.0.0			G	0	eth0

routeend.

