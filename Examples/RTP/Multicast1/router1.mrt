name: eth0	inet_addr: 171.0.0.11
#Groups:	225.0.0.1
MTU: 1500	Metric: 1  BROADCAST MULTICAST

# ethernet card 1 to host 2
name: eth1	inet_addr: 172.0.0.12
#Groups:	225.0.0.1
MTU: 1500	Metric: 1  BROADCAST MULTICAST

# ethernet card 2 to host 3
name: eth2	inet_addr: 173.0.0.13
