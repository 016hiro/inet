$(TARGET): $(OBJS) subdirs Makefile
	

purify: $(OBJS) $(EXTRA_OBJS) subdirs Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

Tester_n.o: Tester_n.cc
	$(CXX) -c $(NEDCOPTS) Tester_n.cc

Tester_n.cc: Tester.ned
	$(NEDC) $(INCLUDE_PATH) Tester.ned

NewBWRequestTester.o: NewBWRequestTester.cc
	$(CXX) -c $(COPTS) NewBWRequestTester.cc

NewRouteTester.o: NewRouteTester.cc
	$(CXX) -c $(COPTS) NewRouteTester.cc


doc: neddoc doxy

neddoc:
	opp_neddoc -a

doxy: doxy.cfg
	doxygen doxy.cfg

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h $(TARGET)$(EXE_SUFFIX)
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done 

