# Name of target to be created (-o option)
TARGET = TCPApp

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# uncomment 1 of the 3 lines to support either serial or parallel operation
KERNEL_LIBS=$(STD_KERNEL_LIBS)
# KERNEL_LIBS=$(PVM_KERNEL_LIBS)
# KERNEL_LIBS=$(MPI_KERNEL_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../../Network/IPv4/Core -I../../Transport/TCP -I../../Util

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with
EXT_DIR_OBJS=

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../../ipsuiteconfig
include ../../ipsuiteconfig

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS= MyTCPApp_n.o mytcpserverproc_n.o procserver_n.o tcpapp_n.o  MyTCPClient.o MyTCPServer.o mytcpserverproc.o procserver.o TCPClient.o TCPServer.o

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) subdirs Makefile
	

purify: $(OBJS) $(EXTRA_OBJS) subdirs Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

MyTCPApp_n.o: MyTCPApp_n.cc
	$(CXX) -c $(NEDCOPTS) MyTCPApp_n.cc

MyTCPApp_n.cc: MyTCPApp.ned
	$(NEDC) $(INCLUDE_PATH) MyTCPApp.ned

mytcpserverproc_n.o: mytcpserverproc_n.cc
	$(CXX) -c $(NEDCOPTS) mytcpserverproc_n.cc

mytcpserverproc_n.cc: mytcpserverproc.ned
	$(NEDC) $(INCLUDE_PATH) mytcpserverproc.ned

procserver_n.o: procserver_n.cc
	$(CXX) -c $(NEDCOPTS) procserver_n.cc

procserver_n.cc: procserver.ned
	$(NEDC) $(INCLUDE_PATH) procserver.ned

tcpapp_n.o: tcpapp_n.cc
	$(CXX) -c $(NEDCOPTS) tcpapp_n.cc

tcpapp_n.cc: tcpapp.ned
	$(NEDC) $(INCLUDE_PATH) tcpapp.ned

MyTCPClient.o: MyTCPClient.cc
	$(CXX) -c $(COPTS) MyTCPClient.cc

MyTCPServer.o: MyTCPServer.cc
	$(CXX) -c $(COPTS) MyTCPServer.cc

mytcpserverproc.o: mytcpserverproc.cc
	$(CXX) -c $(COPTS) mytcpserverproc.cc

procserver.o: procserver.cc
	$(CXX) -c $(COPTS) procserver.cc

TCPClient.o: TCPClient.cc
	$(CXX) -c $(COPTS) TCPClient.cc

TCPServer.o: TCPServer.cc
	$(CXX) -c $(COPTS) TCPServer.cc


doc: neddoc doxy

neddoc:
	opp_neddoc -a

doxy: doxy.cfg
	doxygen doxy.cfg

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h $(TARGET)$(EXE_SUFFIX)
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done 

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc

re-makemake:
	opp_makemake -f  -n -r -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Transport/TCP -I../../Util   #recreate Makefile

re-makemake-m:
	opp_makemake -f -m  -n -r -c ../../ipsuiteconfig -I../../Network/IPv4/Core -I../../Transport/TCP -I../../Util   #recreate Makefile.in


