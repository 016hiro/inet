	$(CXX) -c $(NEDCOPTS) InfoManager_n.cc

InfoManager_n.cc: InfoManager.ned
	$(NEDC) $(INCLUDE_PATH) InfoManager.ned

LIBTable_n.o: LIBTable_n.cc
